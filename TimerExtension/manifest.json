{
  "manifest_version": 3,
  "name" : "Timer Extension",
  "version" : "1.0",
  "description" : "Hello world extension",

  "icons": {
    "16" :  "icon_copy.png",
    "48" :  "icon_copy.png",
    "128" :  "icon_copy.png"
  },

  "action" : {
    "16" :  "icon_copy.png",
    "24" :  "icon_copy.png",
    "32" :  "icon_copy.png",
    "default_title" : "Timer extension action title",
    "default_popup" : "popup.html"
  },

  "options_page" : "options.html",

  "permissions" : [
    "storage",
    "alarms",
    "notifications"
  ],
  "background" : {
    "service_worker" : "background.js"
  }

}
